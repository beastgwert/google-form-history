{
    "manifest_version": 3,
    "version": "1.0.0",
    "name": "Google Form History",
    "api_endpoints": {
        "upload_url": "https://pe3pi87ox8.execute-api.us-east-1.amazonaws.com/prod/upload-url",
        "retrieve_urls": "https://pe3pi87ox8.execute-api.us-east-1.amazonaws.com/prod/retrieve-urls",
        "delete_url": "https://pe3pi87ox8.execute-api.us-east-1.amazonaws.com/prod/delete-url",
        "upload_submission": "https://pe3pi87ox8.execute-api.us-east-1.amazonaws.com/prod/upload-submission",
        "retrieve_submissions": "https://pe3pi87ox8.execute-api.us-east-1.amazonaws.com/prod/retrieve-submissions"
    },
    "action": {
        "default_popup": "index.html",
        "default_icon": {
            "16": "icon16.png",
            "32": "icon32.png",
            "48": "icon48.png",
            "128": "icon128.png"
        }
    },
    "background": {
        "service_worker": "background.js",
        "type": "module"
    },
    "content_scripts": [
        {
            "matches": ["https://docs.google.com/forms/*"],
            "js": ["form-capture-script.js"],
            "run_at": "document_idle"
        }
    ],
    "host_permissions": [
        "https://docs.google.com/forms/*"
    ],
    "permissions": [
        "tabs",
        "scripting",
        "activeTab",
        "storage"
    ]
}
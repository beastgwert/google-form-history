{
    "manifest_version": 3,
    "version": "1.0.0",
    "name": "Google Form History",
    "api_endpoints": {
        "upload_url": "https://pe3pi87ox8.execute-api.us-east-1.amazonaws.com/prod/upload-url",
        "retrieve_urls": "https://pe3pi87ox8.execute-api.us-east-1.amazonaws.com/prod/retrieve-urls",
        "delete_url": "https://pe3pi87ox8.execute-api.us-east-1.amazonaws.com/prod/delete-url"
    },
    "action": {
        "default_popup": "index.html"
    },
    "background": {
        "service_worker": "background.js",
        "type": "module"
    },
    "host_permissions": [
        "https://docs.google.com/forms/*"
    ],
    "permissions": [
        "tabs",
        "scripting",
        "activeTab",
        "storage"
    ]
}